{
  "name": "nouislider",
  "version": "11.1.0",
  "main": "distribute/nouislider",
  "style": "distribute/nouislider.min.css",
  "license": "WTFPL",
  "scripts": {
    "format": "prettier src/nouislider.js --write --print-width=120",
    "lint": "eslint src/nouislider.js",
    "_write": "uglifyjs src/nouislider.js --output distribute/nouislider.js",
    "_write-compress": "uglifyjs src/nouislider.js --compress --mangle --output distribute/nouislider.min.js",
    "_build_js": "npm run format && npm run _write && npm run _write-compress"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "prettier": "^1.14.2",
    "simulant": "0.1.5",
    "uglify-js": "^3.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/leongersen/noUiSlider.git"
  },
  "files": [
    "distribute"
  ]
}
